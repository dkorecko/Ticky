@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html class="text-app-text h-full w-full" lang="en" data-theme="">
    <head>
        <meta charset="utf-8" />
        <base href="~/" />
        <link rel="preload" href="fonts/pxiGyp8kv8JHgFVrLPTucHtA.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLFj_Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLDz8Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiEyp8kv8JHgFVrJJfecg.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLGT9Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLEj6Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLCz7Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLDD4Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <link rel="preload" href="fonts/pxiByp8kv8JHgFVrLBT5Z1xlFQ.woff2" as="font" type="font/woff2" crossorigin>
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link href="css/app.css" rel="stylesheet" />
        <link rel="stylesheet" href="fonts/Poppins.css">
        <link href="Ticky.Web.styles.css" rel="stylesheet" />
        <link rel="icon" type="image/png" href="./images/favicon.ico"/>
        <link rel="stylesheet" href="font-awesome/css/fontawesome.min.css">
        <link rel="stylesheet" href="font-awesome/css/solid.min.css">
        <script src="js/main.js"></script>
        <title>Auth - @Constants.APP_NAME</title>
    </head>
    <body class="auth-bg">
        <div class="hidden p-8 lg:flex">
            <div class="flex w-full flex-col justify-center space-y-4">
                <img src="/assets/illustration-light.svg"
                     alt="Light illustration"
                     class="mx-auto w-2/3 self-center dark:hidden" />

                <img src="/assets/illustration-dark.svg"
                     alt="Dark illustration"
                     class="mx-auto hidden w-2/3 self-center dark:block" />

                <h4 class="mx-auto w-max">Organize. Share. Collaborate.</h4>
            </div>
        </div>
        <div class="auth-card">
            @RenderBody()
        </div>
    </body>

    <script src="js/darkTheme.js" type="text/javascript"></script>
</html>